Host bastionNSO
  User ubuntu
  HostName <ip add>
  IdentityFile ~/.ssh/id_rsa
  UserKnownHostsFile=~/dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no

Host HAproxy
  User ubuntu
  HostName <ip add>
  IdentityFile ~/.ssh/id_rsa
  UserKnownHostsFile=~/dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no

Host devA
  User ubuntu
  HostName <ip add>
  ProxyJump BastionNSO
  StrictHostKeyChecking no
  PasswordAuthentication no

Host devB
  User ubuntu
  HostName <ip add>
  ProxyJump BastionNSO
  StrictHostKeyChecking no
  PasswordAuthentication no

Host devC
  User ubuntu
  HostName <ip add>
  ProxyJump BastionNSO
  StrictHostKeyChecking no
  PasswordAuthentication no

